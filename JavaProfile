package mystry1;



public class Arrays {
	
int a[]= {3,4,5,33,2,24};
int max,min;
int i;
public void amax() {
	int max=a[0];

for(i=0;i<a.length;i++) {
	if(max<a[i])
		max=a[i];
}
System.out.println(max);
}
public void amin(){
	int min=a[0];
	for(i=0;i<a.length;i++) {
		if(min>a[i])
			min=a[i];
}
	System.out.println(min);
}
public static void main(String[] args) {
	Arrays arrayOne=new Arrays();
	arrayOne.amax();
	arrayOne.amin();
	
}

}
